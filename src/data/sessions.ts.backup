  { title: "\"t\": \"Responsible AI for Health: Governance, Implementation and Investment Considerations\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Responsible AI for Health: Governance, Implementation and Investment Considerations\",", knowledgePartners: [], topics: ["Healthcare & Biotech","AI Governance & Regulation","AI Safety & Responsible AI"], dayCode: "Mon16" },
  { title: "\"d\": \"This session will explore real-world experiences in implementing AI in health systems, highlighting key governance and regulatory challenges alongside enabling factors such as data systems, partnerships, leadership, and financing. Drawing on country insights and aligned with global AI governance and normative processes, the session will advance dialogue on ethical, equitable, and people-centred deployment. It will conclude with shared priorities and practical pathways for partnership and investment to support responsible scale-up.\",", dateLabel: "Thu, Feb 19", dateISO: "2026-02-19", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Bharat Mandapam\""], description: "\"d\": \"This session will explore real-world experiences in implementing AI in health systems, highlighting key governance and regulatory challenges alongside enabling factors such as data systems, partnerships, leadership, and financing. Drawing on country insights and aligned with global AI governance and normative processes, the session will advance dialogue on ethical, equitable, and people-centred deployment. It will conclude with shared priorities and practical pathways for partnership and investment to support responsible scale-up.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Global Cooperation & Diplomacy","AI Governance & Regulation","AI Safety & Responsible AI","Data Governance & Open Data"], dayCode: "Thu19" },
