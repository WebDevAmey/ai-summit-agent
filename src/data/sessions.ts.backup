  { title: "\"t\": \"Democratizing AI Compute and Digital Data Infrastructures\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Democratizing AI Compute and Digital Data Infrastructures\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"d\": \"This panel will examine how governments and partners can expand equitable access to the core enablers of AI ecosystems, particularly compute and digital data infrastructure to support inclusive, sustainable innovation. It will discuss practical models such as shared compute, regional infrastructure, and public‚Äìprivate partnerships, alongside investments in skills, data readiness, and governance. The panel will also explore cost-efficient AI approaches and strategies to reduce AI's environmental footprint through energy- and water-efficient infrastructure.\",", dateLabel: "Thu, Feb 19", dateISO: "2026-02-19", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Bharat Mandapam\""], description: "\"d\": \"This panel will examine how governments and partners can expand equitable access to the core enablers of AI ecosystems, particularly compute and digital data infrastructure to support inclusive, sustainable innovation. It will discuss practical models such as shared compute, regional infrastructure, and public‚Äìprivate partnerships, alongside investments in skills, data readiness, and governance. The panel will also explore cost-efficient AI approaches and strategies to reduce AI's environmental footprint through energy- and water-efficient infrastructure.\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability","AI Governance & Regulation","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Thu19" },
  { title: "\"sp\": \"Chenai Chair, Masakhane African Languages Hub; Faith Waithaka, Africa Data Centres Association; Ronnie Chatterji, OpenAI; Sangbu Kim, World Bank; Sanjay Jain, Gates Foundation; Saurabh Garg, MoSPI, GoI; Yan LeCun, AMILabs\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5670\""], description: "\"sp\": \"Chenai Chair, Masakhane African Languages Hub; Faith Waithaka, Africa Data Centres Association; Ronnie Chatterji, OpenAI; Sangbu Kim, World Bank; Sanjay Jain, Gates Foundation; Saurabh Garg, MoSPI, GoI; Yan LeCun, AMILabs\",", knowledgePartners: [], topics: ["Multilingual & Indic Language AI","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"t\": \"Building AI Readiness: From Compute to Capability\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Building AI Readiness: From Compute to Capability\",", knowledgePartners: [], topics: ["AI Infrastructure & Compute"], dayCode: "Mon16" },
  { title: "\"d\": \"The proliferation of Generative AI, driven by the advent of Large Language Models (LLMs), Visual Language Models (VLMs), and Agentic workflows, has fundamentally transformed the requirements for high-performance computing infrastructure. For the users of the IndiaAI compute portal and the broader ecosystem, the selection of Graphics Processing Units (GPUs) has evolved from a simple metric of peak floating-point performance (FLOPS) into a complex optimization challenge involving memory bandwidth, interconnect topology, and architectural specialization.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 A\""], description: "\"d\": \"The proliferation of Generative AI, driven by the advent of Large Language Models (LLMs), Visual Language Models (VLMs), and Agentic workflows, has fundamentally transformed the requirements for high-performance computing infrastructure. For the users of the IndiaAI compute portal and the broader ecosystem, the selection of Graphics Processing Units (GPUs) has evolved from a simple metric of peak floating-point performance (FLOPS) into a complex optimization challenge involving memory bandwidth, interconnect topology, and architectural specialization.\",", knowledgePartners: [], topics: ["Multilingual & Indic Language AI","Frontier AI & Foundation Models","AI Infrastructure & Compute"], dayCode: "Mon16" },
