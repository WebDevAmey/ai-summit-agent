  { title: "\"t\": \"Data, People, and Pre-Empting Mass Exclusion: Building Ethical AI as Digital Public Infrastructure\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Data, People, and Pre-Empting Mass Exclusion: Building Ethical AI as Digital Public Infrastructure\",", knowledgePartners: [], topics: ["Digital Public Infrastructure","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"d\": \"This is a focused session examining how AI in public services can unintentionally exclude vulnerable communities and how such risks can be anticipated and mitigated. Anchored in community-centric design and inclusive data governance, the discussion will connect grassroots realities with ethics, policy, and safety perspectives, positioning AI as a transparent, accountable, and people-first Digital Public Infrastructure that enables social empowerment rather than mass exclusion.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 8\""], description: "\"d\": \"This is a focused session examining how AI in public services can unintentionally exclude vulnerable communities and how such risks can be anticipated and mitigated. Anchored in community-centric design and inclusive data governance, the discussion will connect grassroots realities with ethics, policy, and safety perspectives, positioning AI as a transparent, accountable, and people-first Digital Public Infrastructure that enables social empowerment rather than mass exclusion.\",", knowledgePartners: [], topics: ["AI Governance & Regulation","AI Safety & Responsible AI","Digital Public Infrastructure","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
