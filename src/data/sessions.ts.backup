  { title: "\"t\": \"Exploring a Regulatory Framework for Open Data\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Exploring a Regulatory Framework for Open Data\",", knowledgePartners: [], topics: ["Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"d\": \"Open data is becoming a core pillar of the global AI ecosystem, enabling innovation, evidence-based policymaking, and improved public service delivery. While high-quality datasets are essential for advanced AI, their governance poses complex challenges. This panel examines the opportunities and challenges presented by executive and sector-specific measures, and statutory frameworks for open data. Bringing together policymakers, legal experts, and industry the discussion will explore balancing openness with safeguards, and innovation with accountability.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 A\""], description: "\"d\": \"Open data is becoming a core pillar of the global AI ecosystem, enabling innovation, evidence-based policymaking, and improved public service delivery. While high-quality datasets are essential for advanced AI, their governance poses complex challenges. This panel examines the opportunities and challenges presented by executive and sector-specific measures, and statutory frameworks for open data. Bringing together policymakers, legal experts, and industry the discussion will explore balancing openness with safeguards, and innovation with accountability.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Governance & Regulation","Data Governance & Open Data","Enterprise & Industry AI"], dayCode: "Mon16" },
