  { title: "\"t\": \"Sovereign AI for National Security : India's Path to digital Sovereignty\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Sovereign AI for National Security : India's Path to digital Sovereignty\",", knowledgePartners: [], topics: ["Cybersecurity & National Security"], dayCode: "Mon16" },
