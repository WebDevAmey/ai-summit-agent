  { title: "\"t\": \"AI for Social Good: Impact that Works\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI for Social Good: Impact that Works\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"This day-long research and policy symposium will convene senior policymakers, leading researchers, and practitioners to examine how AI can meaningfully improve public service delivery towards an AI ecosystem that maximises social good. Participants will explore evidence from rigorous impact evaluations of AI innovations applied across education, health, labour, agriculture, governance, and climate ‚Äì highlighting what works, why it works, and what must change for transparent, fair, and responsible scale-up that delivers sustained real-world impact.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L2 Audi 2\""], description: "\"d\": \"This day-long research and policy symposium will convene senior policymakers, leading researchers, and practitioners to examine how AI can meaningfully improve public service delivery towards an AI ecosystem that maximises social good. Participants will explore evidence from rigorous impact evaluations of AI innovations applied across education, health, labour, agriculture, governance, and climate ‚Äì highlighting what works, why it works, and what must change for transparent, fair, and responsible scale-up that delivers sustained real-world impact.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Agriculture & Food Systems","Climate, Energy & Sustainability","Education & Skilling","AI Governance & Regulation","AI Safety & Responsible AI"], dayCode: "Mon16" },
