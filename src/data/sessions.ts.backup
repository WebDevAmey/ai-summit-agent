  { title: "\"t\": \"Fintech for All: Democratizing Financial Access Through AI and Human Capital Development\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Fintech for All: Democratizing Financial Access Through AI and Human Capital Development\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","Finance & Fintech"], dayCode: "Mon16" },
  { title: "\"d\": \"This session explores how AI infrastructure drives equitable growth across fintech ecosystems in India and the Global South. Panelists examine how scalable platforms enable multilingual interfaces and risk models serving diverse populations. The discussion covers GPU-accelerated computing powering real-time fraud detection and personalized banking for underserved communities. Examples demonstrate how democratizing compute access and workforce skilling create economic opportunities and financial inclusion, operationalizing the People and Progress Sutras through the Inclusion for Social Empowerment Chakra.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 6\""], description: "\"d\": \"This session explores how AI infrastructure drives equitable growth across fintech ecosystems in India and the Global South. Panelists examine how scalable platforms enable multilingual interfaces and risk models serving diverse populations. The discussion covers GPU-accelerated computing powering real-time fraud detection and personalized banking for underserved communities. Examples demonstrate how democratizing compute access and workforce skilling create economic opportunities and financial inclusion, operationalizing the People and Progress Sutras through the Inclusion for Social Empowerment Chakra.\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","Finance & Fintech","Multilingual & Indic Language AI","Education & Skilling","Women, Children & Social Impact","Global Cooperation & Diplomacy","AI Infrastructure & Compute"], dayCode: "Mon16" },
