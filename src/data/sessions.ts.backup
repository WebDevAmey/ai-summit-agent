  { title: "\"t\": \"Enterprise-Grade Responsible AI for India and the Global South\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Enterprise-Grade Responsible AI for India and the Global South\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Safety & Responsible AI","Enterprise & Industry AI"], dayCode: "Mon16" },
