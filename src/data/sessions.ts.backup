  { title: "\"t\": \"The Innovation Beneath AI: The U-India Partnership powering the AI Era\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"The Innovation Beneath AI: The U-India Partnership powering the AI Era\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"AI is driving a creative destruction in how the world builds infrastructure, including energy, critical minerals, compute, and data centres. Countries are increasingly building this together, reflected in policy signals around rare earth supply chains and large-scale investments in clean energy to support AI. Entrepreneurs and early-stage backers are supporting the next wave of large-scale infrastructure builds, while national AI strategies continue to shape the broader direction of development.\",", dateLabel: "Wed, Feb 18", dateISO: "2026-02-18", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Bharat Mandapam\""], description: "\"d\": \"AI is driving a creative destruction in how the world builds infrastructure, including energy, critical minerals, compute, and data centres. Countries are increasingly building this together, reflected in policy signals around rare earth supply chains and large-scale investments in clean energy to support AI. Entrepreneurs and early-stage backers are supporting the next wave of large-scale infrastructure builds, while national AI strategies continue to shape the broader direction of development.\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability","Creative AI & Media","AI Governance & Regulation","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Wed18" },
