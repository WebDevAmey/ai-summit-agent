  { title: "\"t\": \"Welfare for All: Ensuring Equitable AI Growth Across the World's Largest and Oldest Democracies\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Welfare for All: Ensuring Equitable AI Growth Across the World's Largest and Oldest Democracies\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses"], dayCode: "Mon16" },
  { title: "\"d\": \"This high-level panel will convene World's largest AI practitioners from Google, Apple, Microsoft, Dell, and others will explore how the world's biggest tech companies can work with governments to achieve democratic access to AI on a global scale. With India and the U.S. as democratic anchors, the session will highlight practical pathways to ensure AI-driven growth delivers broad-based welfare and equal opportunity for all.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 9\""], description: "\"d\": \"This high-level panel will convene World's largest AI practitioners from Google, Apple, Microsoft, Dell, and others will explore how the world's biggest tech companies can work with governments to achieve democratic access to AI on a global scale. With India and the U.S. as democratic anchors, the session will highlight practical pathways to ensure AI-driven growth delivers broad-based welfare and equal opportunity for all.\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","Global Cooperation & Diplomacy"], dayCode: "Mon16" },
  { title: "\"sp\": \"Mr. Julian Waits, Rapid7; Mr. Wilson White, Google\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Agentic AI Roundtable\""], description: "\"sp\": \"Mr. Julian Waits, Rapid7; Mr. Wilson White, Google\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"This roundtable will convene senior industry leaders and government officials to examine governance, interoperability, and responsible deployment challenges posed by emerging agentic AI systems capable of autonomous decision-making and adaptive behaviour. Grounded in real-world use cases, the discussion will focus on cybersecurity, enterprise productivity, and autonomous systems. Participants will explore risk-based governance, secure-by-design principles, and the role of open, industry-led technical standards in enabling safe, scalable, and interoperable agentic AI across borders.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 15\""], description: "\"d\": \"This roundtable will convene senior industry leaders and government officials to examine governance, interoperability, and responsible deployment challenges posed by emerging agentic AI systems capable of autonomous decision-making and adaptive behaviour. Grounded in real-world use cases, the discussion will focus on cybersecurity, enterprise productivity, and autonomous systems. Participants will explore risk-based governance, secure-by-design principles, and the role of open, industry-led technical standards in enabling safe, scalable, and interoperable agentic AI across borders.\",", knowledgePartners: [], topics: ["Cybersecurity & National Security","AI Governance & Regulation","AI Safety & Responsible AI","Enterprise & Industry AI"], dayCode: "Mon16" },
