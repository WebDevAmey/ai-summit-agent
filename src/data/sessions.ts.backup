  { title: "\"t\": \"Diagnosing the Future: AI Transforming Healthcare Systems\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Diagnosing the Future: AI Transforming Healthcare Systems\",", knowledgePartners: [], topics: ["Healthcare & Biotech"], dayCode: "Mon16" },
  { title: "\"d\": \"Artificial Intelligence is reshaping India's healthcare system, beginning with medical technologies and diagnostics and extending to hospital operations, clinical decision support, research, and preventive care. AI-enabled imaging, IVDs, and analytics are improving accuracy, access, and system efficiency, particularly in resource-constrained settings. This session will examine policy, governance, and collaboration pathways to responsibly scale AI across diagnostics, care delivery and research, supporting a resilient and future-ready healthcare ecosystem\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 14\""], description: "\"d\": \"Artificial Intelligence is reshaping India's healthcare system, beginning with medical technologies and diagnostics and extending to hospital operations, clinical decision support, research, and preventive care. AI-enabled imaging, IVDs, and analytics are improving accuracy, access, and system efficiency, particularly in resource-constrained settings. This session will examine policy, governance, and collaboration pathways to responsibly scale AI across diagnostics, care delivery and research, supporting a resilient and future-ready healthcare ecosystem\",", knowledgePartners: [], topics: ["Healthcare & Biotech","AI Governance & Regulation"], dayCode: "Mon16" },
