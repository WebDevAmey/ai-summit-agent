  { title: "\"t\": \"Building Trust: Digital Infrastructure Fit for the AI Era\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Building Trust: Digital Infrastructure Fit for the AI Era\",", knowledgePartners: [], topics: ["AI Infrastructure & Compute"], dayCode: "Mon16" },
  { title: "\"d\": \"AI is reshaping how sovereignty is understood for individuals, communities, and nations. While governments played a major role in building the early internet, private actors have also shaped the digital economy. Today, AI development is largely led by the private sector, raising questions about how to align innovation with broader societal goals. This panel will explore how to build trustworthy infrastructure through public-private alignment and governance approaches that prioritize users and accountability. Key takeaways will be distilled across international policy platforms.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 A\""], description: "\"d\": \"AI is reshaping how sovereignty is understood for individuals, communities, and nations. While governments played a major role in building the early internet, private actors have also shaped the digital economy. Today, AI development is largely led by the private sector, raising questions about how to align innovation with broader societal goals. This panel will explore how to build trustworthy infrastructure through public-private alignment and governance approaches that prioritize users and accountability. Key takeaways will be distilled across international policy platforms.\",", knowledgePartners: [], topics: ["AI Governance & Regulation","AI Infrastructure & Compute"], dayCode: "Mon16" },
  { title: "\"sp\": \"Dr. Arvind Gupta, Digital India Foundation; Dr. Tomicah Tillemann, Project Liberty Institute; Robert Opp, United Nations Development Programme (UNDP); Sarah Nicole, Project Liberty Institute; Vidisha Mishra, Global Solutions Initiative\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"sp\": \"Dr. Arvind Gupta, Digital India Foundation; Dr. Tomicah Tillemann, Project Liberty Institute; Robert Opp, United Nations Development Programme (UNDP); Sarah Nicole, Project Liberty Institute; Vidisha Mishra, Global Solutions Initiative\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy"], dayCode: "Mon16" },
