  { title: "\"t\": \"Faculty Futures 2035: Global Roadmap for Human-Centred AI Education\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Faculty Futures 2035: Global Roadmap for Human-Centred AI Education\",", knowledgePartners: [], topics: ["Education & Skilling","Global Cooperation & Diplomacy"], dayCode: "Mon16" },
  { title: "\"d\": \"As AI reshapes teaching, assessment, and governance, higher education faces a shared challenge: preparing faculty for ethical, inclusive, and human-centered engagement. Following pre-summit workshops conducted in Canada, Australia, and India, the Faculty Futures 2035 panel convenes partners at the India AI Impact Summit 2026 to synthesize insights, surface cross-context lessons, and present a forward-looking roadmap to guide institutional strategy for the decade ahead.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 14\""], description: "\"d\": \"As AI reshapes teaching, assessment, and governance, higher education faces a shared challenge: preparing faculty for ethical, inclusive, and human-centered engagement. Following pre-summit workshops conducted in Canada, Australia, and India, the Faculty Futures 2035 panel convenes partners at the India AI Impact Summit 2026 to synthesize insights, surface cross-context lessons, and present a forward-looking roadmap to guide institutional strategy for the decade ahead.\",", knowledgePartners: [], topics: ["Education & Skilling","AI Governance & Regulation"], dayCode: "Mon16" },
