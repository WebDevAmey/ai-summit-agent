  { title: "\"t\": \"Harnessing AI for Equitable and Resilient Health Systems\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Harnessing AI for Equitable and Resilient Health Systems\",", knowledgePartners: [], topics: ["Healthcare & Biotech"], dayCode: "Mon16" },
  { title: "\"d\": \"This session convenes policymakers, industry, technologists, and public health experts to explore how AI can be responsibly scaled to deliver health system impact. With reference to case studies and innovation platforms, it will examine practical pathways for moving from pilots to sustainable, system-level adoption. The session will explore governance, standards, and ethical considerations for responsible AI adoption and identify pathways for inclusive and equitable AI adoption, particularly in low- and middle-income countries.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 A\""], description: "\"d\": \"This session convenes policymakers, industry, technologists, and public health experts to explore how AI can be responsibly scaled to deliver health system impact. With reference to case studies and innovation platforms, it will examine practical pathways for moving from pilots to sustainable, system-level adoption. The session will explore governance, standards, and ethical considerations for responsible AI adoption and identify pathways for inclusive and equitable AI adoption, particularly in low- and middle-income countries.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","AI Governance & Regulation","AI Safety & Responsible AI","Enterprise & Industry AI"], dayCode: "Mon16" },
