  { title: "\"t\": \"AI Commons for the Global South: Data, Models and Compute for Half of Humanity\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI Commons for the Global South: Data, Models and Compute for Half of Humanity\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"d\": \"AI infrastructure, encompassing compute, data, and models, will determine who is able to build, deploy, and govern AI over the coming decade. At present, access to these foundational resources remains uneven and highly concentrated, constraining meaningful participation across much of the Global South. This session, organised by Factly and Meta, examines how an AI Commons can be developed through shared compute frameworks, AI-ready open data systems, and interoperable model and tooling ecosystems aligned with globally recognised standards.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 10\""], description: "\"d\": \"AI infrastructure, encompassing compute, data, and models, will determine who is able to build, deploy, and govern AI over the coming decade. At present, access to these foundational resources remains uneven and highly concentrated, constraining meaningful participation across much of the Global South. This session, organised by Factly and Meta, examines how an AI Commons can be developed through shared compute frameworks, AI-ready open data systems, and interoperable model and tooling ecosystems aligned with globally recognised standards.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
