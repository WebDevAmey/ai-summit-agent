  { title: "\"t\": \"Women at the Frontline of AI: Reimagining Skilling for India's Community Health Workforce\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Women at the Frontline of AI: Reimagining Skilling for India's Community Health Workforce\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Education & Skilling","Women, Children & Social Impact"], dayCode: "Mon16" },
  { title: "\"d\": \"This session examines how AI can strengthen India's women-led community health workforce through inclusive skilling and real-world deployment. Drawing on large-scale use of LLM-powered tools with ASHAs and ANMs across multiple states, the session explores productivity gains, quality of care, and equitable AI access in rural settings. The discussion highlights practical pathways for building a scalable, gender-responsive, AI-enabled frontline workforce in India and globally.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 A\""], description: "\"d\": \"This session examines how AI can strengthen India's women-led community health workforce through inclusive skilling and real-world deployment. Drawing on large-scale use of LLM-powered tools with ASHAs and ANMs across multiple states, the session explores productivity gains, quality of care, and equitable AI access in rural settings. The discussion highlights practical pathways for building a scalable, gender-responsive, AI-enabled frontline workforce in India and globally.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Frontier AI & Foundation Models","Education & Skilling","Women, Children & Social Impact","Global Cooperation & Diplomacy"], dayCode: "Mon16" },
