  { title: "\"t\": \"AI governance in the Age of Powerful AI - IInternational Perspectives and the Code of Practice\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI governance in the Age of Powerful AI - IInternational Perspectives and the Code of Practice\",", knowledgePartners: [], topics: ["AI Governance & Regulation"], dayCode: "Mon16" },
