  { title: "\"t\": \"AI and the Future of Work: Employability, Skills, and Labour Market Transformation\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI and the Future of Work: Employability, Skills, and Labour Market Transformation\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"This panel examines how Artificial Intelligence is reshaping labour markets, from job creation and skill transitions to education and policy responses. Bringing together government, research, industry, and academia, the discussion explores AI-enabled employment platforms, emerging skill demands, workforce strategies, and how education systems can evolve to prepare workers for an AI-driven economy.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 14\""], description: "\"d\": \"This panel examines how Artificial Intelligence is reshaping labour markets, from job creation and skill transitions to education and policy responses. Bringing together government, research, industry, and academia, the discussion explores AI-enabled employment platforms, emerging skill demands, workforce strategies, and how education systems can evolve to prepare workers for an AI-driven economy.\",", knowledgePartners: [], topics: ["Education & Skilling","AI Governance & Regulation","Enterprise & Industry AI"], dayCode: "Mon16" },
