  { title: "\"t\": \"Democratizing AI Requires a Free and Open Internet\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Democratizing AI Requires a Free and Open Internet\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses"], dayCode: "Mon16" },
  { title: "\"d\": \"As AI becomes deeply embedded in economies and societies, the key question is who has access and who benefits. This session brings thought leaders to discuss how true AI democratization depends on preserving the Internet as a global platform rather than fragmented walled gardens. It will explore how open-source ecosystems, data commons, and distributed compute powered by an open Internet can enable startups and developing nations, especially India, to realize inclusive \\\"AI for All\\\" ambitions.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 6\""], description: "\"d\": \"As AI becomes deeply embedded in economies and societies, the key question is who has access and who benefits. This session brings thought leaders to discuss how true AI democratization depends on preserving the Internet as a global platform rather than fragmented walled gardens. It will explore how open-source ecosystems, data commons, and distributed compute powered by an open Internet can enable startups and developing nations, especially India, to realize inclusive \\\"AI for All\\\" ambitions.\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","Startups & Venture Capital","Global Cooperation & Diplomacy","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
