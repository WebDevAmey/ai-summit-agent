  { title: "\"t\": \"Trust and Safety as Engines for Growth and Innovation\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Trust and Safety as Engines for Growth and Innovation\",", knowledgePartners: [], topics: ["AI Safety & Responsible AI"], dayCode: "Mon16" },
  { title: "\"d\": \"Around the world governments and businesses agree: trust is the key for accelerating AI adoption. While research has extensively mapped why trust in AI matters, far less attention has been given to how trust will be implemented, including issues around the people, skills and systems required to operationalize trust and safety at scale.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 7\""], description: "\"d\": \"Around the world governments and businesses agree: trust is the key for accelerating AI adoption. While research has extensively mapped why trust in AI matters, far less attention has been given to how trust will be implemented, including issues around the people, skills and systems required to operationalize trust and safety at scale.\",", knowledgePartners: [], topics: ["AI Safety & Responsible AI"], dayCode: "Mon16" },
