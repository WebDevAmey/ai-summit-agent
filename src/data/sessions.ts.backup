  { title: "\"t\": \"Multilingual AI in Universities - Advancing Inclusion Through UA and Stakeholder Insights\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Multilingual AI in Universities - Advancing Inclusion Through UA and Stakeholder Insights\",", knowledgePartners: [], topics: ["Multilingual & Indic Language AI","Women, Children & Social Impact"], dayCode: "Mon16" },
  { title: "\"d\": \"As digital technologies become central to education, linguistic accessibility emerges as a critical equity issue. This study examines how universities enable multilingual participation through UA. Drawing on Diffusion Innovation Theory (DIT) and Technology Acceptance Model (TAM), this investigation reviews stakeholder experiences across universities in Asia and Europe using an innovative AI-assisted interview methodology. The findings also revealed critical gaps in universities' capacity to advance UN SDG 4/9/10/17 through inclusive digital infrastructure while presenting billions of dollars opportunity.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room No. 6\""], description: "\"d\": \"As digital technologies become central to education, linguistic accessibility emerges as a critical equity issue. This study examines how universities enable multilingual participation through UA. Drawing on Diffusion Innovation Theory (DIT) and Technology Acceptance Model (TAM), this investigation reviews stakeholder experiences across universities in Asia and Europe using an innovative AI-assisted interview methodology. The findings also revealed critical gaps in universities' capacity to advance UN SDG 4/9/10/17 through inclusive digital infrastructure while presenting billions of dollars opportunity.\",", knowledgePartners: [], topics: ["Multilingual & Indic Language AI","Education & Skilling","AI Infrastructure & Compute"], dayCode: "Mon16" },
