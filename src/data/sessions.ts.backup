  { title: "\"t\": \"Quality Control of AI Applications in Healthcare\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Quality Control of AI Applications in Healthcare\",", knowledgePartners: [], topics: ["Healthcare & Biotech"], dayCode: "Mon16" },
  { title: "\"d\": \"Deploy AI models in healthcare through phased pilots, using clinically validated datasets and bias audits. Establish strict data governance, patient consent, and privacy compliance. Implement human-in-the-loop oversight for all clinical decisions. Mandate regulatory approvals, model explainability, and documentation. Conduct continuous performance monitoring, post-deployment audits, and retraining. Define clear accountability, cybersecurity safeguards, and standardized quality benchmarks to ensure safety, accuracy, transparency, and trust in real-world clinical use.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 B\""], description: "\"d\": \"Deploy AI models in healthcare through phased pilots, using clinically validated datasets and bias audits. Establish strict data governance, patient consent, and privacy compliance. Implement human-in-the-loop oversight for all clinical decisions. Mandate regulatory approvals, model explainability, and documentation. Conduct continuous performance monitoring, post-deployment audits, and retraining. Define clear accountability, cybersecurity safeguards, and standardized quality benchmarks to ensure safety, accuracy, transparency, and trust in real-world clinical use.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Cybersecurity & National Security","AI Governance & Regulation","AI Safety & Responsible AI","Data Governance & Open Data"], dayCode: "Mon16" },
