  { title: "\"t\": \"Unlocking the Future: A Pact Between Tech Giants and the Next Generation\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Unlocking the Future: A Pact Between Tech Giants and the Next Generation\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"Adolescents are engaging with AI more frequently and in increasingly emotional ways during a critical developmental stage that requires specific safeguards, especially in the presence of anthropomorphic design. This panel introduces an international multistakeholder coalition leading global efforts on this issue. It presents early findings from this transdisciplinary initiative, grounded in neuroscience and AI safety research, and highlights the roles and responsibilities of stakeholders in creating beneficial, child-centered, and safe AI\",", dateLabel: "Wed, Feb 18", dateISO: "2026-02-18", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Bharat Mandapam\""], description: "\"d\": \"Adolescents are engaging with AI more frequently and in increasingly emotional ways during a critical developmental stage that requires specific safeguards, especially in the presence of anthropomorphic design. This panel introduces an international multistakeholder coalition leading global efforts on this issue. It presents early findings from this transdisciplinary initiative, grounded in neuroscience and AI safety research, and highlights the roles and responsibilities of stakeholders in creating beneficial, child-centered, and safe AI\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Safety & Responsible AI"], dayCode: "Wed18" },
  { title: "\"sp\": \"Adrien Abecassis, Paris Peace Forum, iRAISE; Anne-Sophie Seret, everyone.ai and iRAISE; Asophie Seret, everyone.ai; Dr Mathilde Cerioli, everyone.ai; Lakshmi Pratury, INK talks; Sampurna Behura, India Child Protection; Stuart Russel, University of California/ Berkeley; Stuart Russel, University of California/ Berkeley; Vithika Yadav, Youth Representative\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5821\""], description: "\"sp\": \"Adrien Abecassis, Paris Peace Forum, iRAISE; Anne-Sophie Seret, everyone.ai and iRAISE; Asophie Seret, everyone.ai; Dr Mathilde Cerioli, everyone.ai; Lakshmi Pratury, INK talks; Sampurna Behura, India Child Protection; Stuart Russel, University of California/ Berkeley; Stuart Russel, University of California/ Berkeley; Vithika Yadav, Youth Representative\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"t\": \"Genomics, AI, and the Future of Health: Data Visitation to Empower the Global South\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Genomics, AI, and the Future of Health: Data Visitation to Empower the Global South\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Global Cooperation & Diplomacy","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"d\": \"Realizing global precision medicine requires infrastructure enabling countries to collaborate on AI-driven genomics. Yet data sovereignty rules prevent collective use of genomic datasets, widening the Global AI Divide. Data visitation‚Äîwhere algorithms travel securely to data rather than data moving to researchers‚Äîoffers a path forward. This workshop convenes policymakers, technologists, and public health leaders to explore governance, standards, and technologies, showcasing how emerging tools translate policy into deployable infrastructure built with the Global South.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 15\""], description: "\"d\": \"Realizing global precision medicine requires infrastructure enabling countries to collaborate on AI-driven genomics. Yet data sovereignty rules prevent collective use of genomic datasets, widening the Global AI Divide. Data visitation‚Äîwhere algorithms travel securely to data rather than data moving to researchers‚Äîoffers a path forward. This workshop convenes policymakers, technologists, and public health leaders to explore governance, standards, and technologies, showcasing how emerging tools translate policy into deployable infrastructure built with the Global South.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Global Cooperation & Diplomacy","AI Governance & Regulation","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"sp\": \"Andrew Soltan, University of Oxford; Binay Panda, Jawaharlal Nehru University; Dawn Chen, Harvard University; Francis Crawley, CODATA International Data Policy Committee; Madhava Jay, OpenMined Foundation; Suchita Ninawe, Department of Biotechnology, GoI; Weibin Liu, Human Genome Project II\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5657\""], description: "\"sp\": \"Andrew Soltan, University of Oxford; Binay Panda, Jawaharlal Nehru University; Dawn Chen, Harvard University; Francis Crawley, CODATA International Data Policy Committee; Madhava Jay, OpenMined Foundation; Suchita Ninawe, Department of Biotechnology, GoI; Weibin Liu, Human Genome Project II\",", knowledgePartners: [], topics: ["Healthcare & Biotech","AI Governance & Regulation","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"t\": \"AI That Works: Evaluating Technology for Social Change\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI That Works: Evaluating Technology for Social Change\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"As AI use cases expand across the social sector, robust evaluation systems are critical to safeguard vulnerable users, improve system performance, and ensure meaningful positive impact. This session addresses the growing need to support development leaders in designing cost-effective AI evaluation pipelines that enhance benefits while minimizing potential harm. It will draw on culturally responsive and context-specific evaluation approaches aimed at improving the safety and reliability of LLMs for diverse, multilingual communities.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 14\""], description: "\"d\": \"As AI use cases expand across the social sector, robust evaluation systems are critical to safeguard vulnerable users, improve system performance, and ensure meaningful positive impact. This session addresses the growing need to support development leaders in designing cost-effective AI evaluation pipelines that enhance benefits while minimizing potential harm. It will draw on culturally responsive and context-specific evaluation approaches aimed at improving the safety and reliability of LLMs for diverse, multilingual communities.\",", knowledgePartners: [], topics: ["Multilingual & Indic Language AI","Frontier AI & Foundation Models","AI Safety & Responsible AI"], dayCode: "Mon16" },
