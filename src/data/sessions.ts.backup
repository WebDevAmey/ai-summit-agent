  { title: "\"t\": \"AI for Democracy: Reimagining Governance in the Age of Intelligence\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI for Democracy: Reimagining Governance in the Age of Intelligence\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","AI Governance & Regulation"], dayCode: "Mon16" },
