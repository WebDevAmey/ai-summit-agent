  { title: "\"t\": \"Redesigning the AI Economy\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Redesigning the AI Economy\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"The main stage session will examine how participatory and ethical data pipelines can support a more inclusive AI ecosystem at national scale. Bringing together policymakers, technologists, researchers, and community representatives, the discussion will explore community-led data creation, evaluation, and model development. The session will highlight how fair compensation, local knowledge, and transparent governance can strengthen AI systems and inform responsible AI policy and practice in India and across the Global Majority.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 8\""], description: "\"d\": \"The main stage session will examine how participatory and ethical data pipelines can support a more inclusive AI ecosystem at national scale. Bringing together policymakers, technologists, researchers, and community representatives, the discussion will explore community-led data creation, evaluation, and model development. The session will highlight how fair compensation, local knowledge, and transparent governance can strengthen AI systems and inform responsible AI policy and practice in India and across the Global Majority.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Governance & Regulation","AI Safety & Responsible AI","Data Governance & Open Data"], dayCode: "Mon16" },
