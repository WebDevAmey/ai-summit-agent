  { title: "\"t\": \"Operating Models for AI Quality Across Billion-User Markets\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Operating Models for AI Quality Across Billion-User Markets\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
