  { title: "\"t\": \"Democratizing Compute Through Regional Infrastructure\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Democratizing Compute Through Regional Infrastructure\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","AI Infrastructure & Compute"], dayCode: "Mon16" },
  { title: "\"d\": \"This session examines infrastructure for achieving gigawatt-scale compute capacity across emerging economies. As nations transition from data generation to domestic processing, the discussion explores building industrial-scale data centers. Panelists demonstrate how advanced GPU architectures and networking, such as NVIDIA's DGX SuperPOD and Grace CPU, enable self-reliant infrastructure across regional hubs. This operationalizes the Democratizing AI Resources Chakra, demonstrating impact through increased compute capacity, reduced latency, and accelerated transformation.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 6\""], description: "\"d\": \"This session examines infrastructure for achieving gigawatt-scale compute capacity across emerging economies. As nations transition from data generation to domestic processing, the discussion explores building industrial-scale data centers. Panelists demonstrate how advanced GPU architectures and networking, such as NVIDIA's DGX SuperPOD and Grace CPU, enable self-reliant infrastructure across regional hubs. This operationalizes the Democratizing AI Resources Chakra, demonstrating impact through increased compute capacity, reduced latency, and accelerated transformation.\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
