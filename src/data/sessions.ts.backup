  { title: "\"kp\": \"U.S. Department of Energy (DOE) National Renewable Energy Laboratory (NREL)\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5584\""], description: "\"kp\": \"U.S. Department of Energy (DOE) National Renewable Energy Laboratory (NREL)\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability"], dayCode: "Mon16" },
  { title: "\"t\": \"AI in Health: Saving Lives at Scale  \",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI in Health: Saving Lives at Scale  \",", knowledgePartners: [], topics: ["Healthcare & Biotech"], dayCode: "Mon16" },
  { title: "\"d\": \"Can AI innovation help in diagnosing silent heart attacks to deliver measurable health gains for underserved populations at scale? Hear from a leading international researcher, and an expert panel, about how rigorous evidence from randomised evaluations can distinguish promising ideas from solutions, and strengthen health systems. Showcasing on-ground evidence, participants will understand what it takes to make AI work in practice - aligning technology with frontline workflows and governance structures, and generating actionable evidence for sustainable improvements in healthcare delivery.\",", dateLabel: "Fri, Feb 20", dateISO: "2026-02-20", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L2 Audi 2\""], description: "\"d\": \"Can AI innovation help in diagnosing silent heart attacks to deliver measurable health gains for underserved populations at scale? Hear from a leading international researcher, and an expert panel, about how rigorous evidence from randomised evaluations can distinguish promising ideas from solutions, and strengthen health systems. Showcasing on-ground evidence, participants will understand what it takes to make AI work in practice - aligning technology with frontline workflows and governance structures, and generating actionable evidence for sustainable improvements in healthcare delivery.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","AI Governance & Regulation"], dayCode: "Fri20" },
