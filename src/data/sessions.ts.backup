  { title: "\"kp\": \"Centre for Digital Transformation of Health (CDTH), The University of Melbourne\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5889\""], description: "\"kp\": \"Centre for Digital Transformation of Health (CDTH), The University of Melbourne\",", knowledgePartners: [], topics: ["Healthcare & Biotech"], dayCode: "Mon16" },
  { title: "\"t\": \"From Guidelines to Ground: Institutional AI Safety for India and the Global South\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"From Guidelines to Ground: Institutional AI Safety for India and the Global South\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Safety & Responsible AI"], dayCode: "Mon16" },
  { title: "\"d\": \"This roundtable convenes government, industry, legal, and diplomatic leaders to translate responsible AI principles into enforceable institutional guardrails for India and the Global South. Participants will examine real-world deployment failures, identify binding constraints in enforcement, audit bandwidth, and procurement, and develop minimum viable safeguards for high-adoption, low-capacity settings. Outputs include an Institutional Safety Case Standard and Red Teaming Checklist. The session bridges policy intent with ground-level implementation realities.\",", dateLabel: "Tue, Feb 17", dateISO: "2026-02-17", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 A\""], description: "\"d\": \"This roundtable convenes government, industry, legal, and diplomatic leaders to translate responsible AI principles into enforceable institutional guardrails for India and the Global South. Participants will examine real-world deployment failures, identify binding constraints in enforcement, audit bandwidth, and procurement, and develop minimum viable safeguards for high-adoption, low-capacity settings. Outputs include an Institutional Safety Case Standard and Red Teaming Checklist. The session bridges policy intent with ground-level implementation realities.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Governance & Regulation","AI Safety & Responsible AI","Enterprise & Industry AI"], dayCode: "Tue17" },
  { title: "\"sp\": \"Abdullah, Zephara AI; Arundhati Banerjee, Nvidia; Dr. Arjun Goswami, Cyril Amarchand Mangaldas; Juan Carlos Rojas Arango, Embassy of Colombia; Kazim Rizvi, The Dialogue; Prof. Sidharth Chauhan, The Cyril Shroff Centre for AI Law and Regulation (JGU); Sidharth Deb, The Quantum Hub\",", dateLabel: "Fri, Feb 20", dateISO: "2026-02-20", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5655\""], description: "\"sp\": \"Abdullah, Zephara AI; Arundhati Banerjee, Nvidia; Dr. Arjun Goswami, Cyril Amarchand Mangaldas; Juan Carlos Rojas Arango, Embassy of Colombia; Kazim Rizvi, The Dialogue; Prof. Sidharth Chauhan, The Cyril Shroff Centre for AI Law and Regulation (JGU); Sidharth Deb, The Quantum Hub\",", knowledgePartners: [], topics: ["Space, Quantum & Emerging Tech","AI Governance & Regulation"], dayCode: "Fri20" },
  { title: "\"d\": \"Generation AI @ 2047 is a high-impact, experience-led session aligned with India's focus on inclusive and scalable AI. Moving beyond policy intent, it explores how AI shows up at India's last mile - shaping outcomes for farmers, classrooms, and first-time job seekers. Hosted in Hindi, it spotlights a state-level AI skilling initiative, offering practical, replicable insights for building an inclusive, AI-ready workforce toward a Viksit Bharat by 2047.\",", dateLabel: "Tue, Feb 17", dateISO: "2026-02-17", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 15\""], description: "\"d\": \"Generation AI @ 2047 is a high-impact, experience-led session aligned with India's focus on inclusive and scalable AI. Moving beyond policy intent, it explores how AI shows up at India's last mile - shaping outcomes for farmers, classrooms, and first-time job seekers. Hosted in Hindi, it spotlights a state-level AI skilling initiative, offering practical, replicable insights for building an inclusive, AI-ready workforce toward a Viksit Bharat by 2047.\",", knowledgePartners: [], topics: ["Education & Skilling","AI Governance & Regulation"], dayCode: "Tue17" },
