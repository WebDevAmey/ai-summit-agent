  { title: "\"t\": \"Reimagining Accessibility of Public Broadcasting Data in the AI Era: Creating Virtuous Production of Data for Local Language LLMs\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Reimagining Accessibility of Public Broadcasting Data in the AI Era: Creating Virtuous Production of Data for Local Language LLMs\",", knowledgePartners: [], topics: ["Multilingual & Indic Language AI","Frontier AI & Foundation Models","Data Governance & Open Data"], dayCode: "Mon16" },
