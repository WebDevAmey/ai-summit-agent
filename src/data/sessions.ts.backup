  { title: "\"t\": \"Multilingual Voice-AI Agents for Crisis Response and Underserved Populations\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Multilingual Voice-AI Agents for Crisis Response and Underserved Populations\",", knowledgePartners: [], topics: ["Multilingual & Indic Language AI"], dayCode: "Mon16" },
  { title: "\"d\": \"When floods strike, clinics overflow, EMIs slip, or civic systems fail, help exists behind apps, portals, and English menus. For millions, that help is unreachable. This session explores Multilingual Voice-AI Agents for crisis response, reaching people by phone, in their language, handling real tasks in real time. This involves no downloads and forms - just voice that works when systems don't, especially for underserved and vulnerable communities.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 14\""], description: "\"d\": \"When floods strike, clinics overflow, EMIs slip, or civic systems fail, help exists behind apps, portals, and English menus. For millions, that help is unreachable. This session explores Multilingual Voice-AI Agents for crisis response, reaching people by phone, in their language, handling real tasks in real time. This involves no downloads and forms - just voice that works when systems don't, especially for underserved and vulnerable communities.\",", knowledgePartners: [], topics: ["Multilingual & Indic Language AI"], dayCode: "Mon16" },
