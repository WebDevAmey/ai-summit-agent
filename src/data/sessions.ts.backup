  { title: "\"t\": \"Decoded: How AI Is Reshaping Work for Women\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Decoded: How AI Is Reshaping Work for Women\",", knowledgePartners: [], topics: ["Women, Children & Social Impact"], dayCode: "Mon16" },
  { title: "\"d\": \"This panel will examine the intersection of AI, gender, and the future of work, within evolving digital economies. The discussion will explore gendered impacts of AI while highlighting design, governance, and workforce strategies that consider women's lived experiences and inform policy-relevant pathways toward inclusive, AI-enabled growth. Building on emerging insights from skilling initiatives, the session will focus on identifying pathways where AI can expand decent work and economic security for women at scale.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 B\""], description: "\"d\": \"This panel will examine the intersection of AI, gender, and the future of work, within evolving digital economies. The discussion will explore gendered impacts of AI while highlighting design, governance, and workforce strategies that consider women's lived experiences and inform policy-relevant pathways toward inclusive, AI-enabled growth. Building on emerging insights from skilling initiatives, the session will focus on identifying pathways where AI can expand decent work and economic security for women at scale.\",", knowledgePartners: [], topics: ["Cybersecurity & National Security","Education & Skilling","Women, Children & Social Impact","AI Governance & Regulation"], dayCode: "Mon16" },
