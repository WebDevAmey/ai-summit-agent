  { title: "\"t\": \"Building Inclusive Futures through AI Literacy for India and the Global South\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Building Inclusive Futures through AI Literacy for India and the Global South\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy"], dayCode: "Mon16" },
  { title: "\"d\": \"This session will examine how AI can be integrated into education in India and the Global South in a responsible, inclusive, and scalable way. It will address the need for foundational AI literacy, clarify what AI literacy means for students, teachers, and parents, and explore practical pathways from curriculum design to classroom implementation. The discussion will also focus on governance, ethics, data privacy, and system-level conditions for safe, large-scale innovation in schools.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 9\""], description: "\"d\": \"This session will examine how AI can be integrated into education in India and the Global South in a responsible, inclusive, and scalable way. It will address the need for foundational AI literacy, clarify what AI literacy means for students, teachers, and parents, and explore practical pathways from curriculum design to classroom implementation. The discussion will also focus on governance, ethics, data privacy, and system-level conditions for safe, large-scale innovation in schools.\",", knowledgePartners: [], topics: ["Education & Skilling","Global Cooperation & Diplomacy","AI Governance & Regulation","AI Safety & Responsible AI","Data Governance & Open Data"], dayCode: "Mon16" },
