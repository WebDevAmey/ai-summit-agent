  { title: "\"t\": \"Planet-Scale Intelligence for Economic Growth: AgriStack, Climate AI, and Foundational Models for Global Impact\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Planet-Scale Intelligence for Economic Growth: AgriStack, Climate AI, and Foundational Models for Global Impact\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability","Global Cooperation & Diplomacy"], dayCode: "Mon16" },
  { title: "\"d\": \"This session explores how planet-scale intelligence built on AI, Earth observation and Digital Public Infrastructure can move from pilots to production and reshape real economies. Anchored in India's experience with AgriStack, it examines climate-smart credit, resilient food systems, and risk-informed public spending, with lessons for the Global South. Speakers will discuss foundational Earth models, governance and validation layers, and the public‚Äìprivate architectures needed to make AI a trusted, shared asset for inclusive growth.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room No. 6\""], description: "\"d\": \"This session explores how planet-scale intelligence built on AI, Earth observation and Digital Public Infrastructure can move from pilots to production and reshape real economies. Anchored in India's experience with AgriStack, it examines climate-smart credit, resilient food systems, and risk-informed public spending, with lessons for the Global South. Speakers will discuss foundational Earth models, governance and validation layers, and the public‚Äìprivate architectures needed to make AI a trusted, shared asset for inclusive growth.\",", knowledgePartners: [], topics: ["Agriculture & Food Systems","Climate, Energy & Sustainability","Global Cooperation & Diplomacy","AI Governance & Regulation","Digital Public Infrastructure","AI Infrastructure & Compute"], dayCode: "Mon16" },
