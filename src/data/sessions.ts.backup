  { title: "\"t\": \"Teacher-Led, Localised and Frugal AI for Equitable Education in the Commonwealth\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Teacher-Led, Localised and Frugal AI for Equitable Education in the Commonwealth\",", knowledgePartners: [], topics: ["Education & Skilling"], dayCode: "Mon16" },
