  { title: "\"t\": \"AI and the Future of Skilling: Strengthening Human Capital and Transforming Higher Education Institutions\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI and the Future of Skilling: Strengthening Human Capital and Transforming Higher Education Institutions\",", knowledgePartners: [], topics: ["Education & Skilling"], dayCode: "Mon16" },
  { title: "\"d\": \"As AI transforms economies, developing strong human capital is increasingly important for workforce readiness. This session on \\\"AI and the Future of Skilling\\\" at the India AI Impact Summit 2026 will convene stakeholders from government, industry, and academia to discuss approaches to building an AI-ready workforce. The discussion will explore policy pathways, industry‚Äìacademia collaboration, institutional AI readiness, future-focused skilling frameworks, and capacity-building across higher education to support employability, innovation, and broader adoption.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 9\""], description: "\"d\": \"As AI transforms economies, developing strong human capital is increasingly important for workforce readiness. This session on \\\"AI and the Future of Skilling\\\" at the India AI Impact Summit 2026 will convene stakeholders from government, industry, and academia to discuss approaches to building an AI-ready workforce. The discussion will explore policy pathways, industry‚Äìacademia collaboration, institutional AI readiness, future-focused skilling frameworks, and capacity-building across higher education to support employability, innovation, and broader adoption.\",", knowledgePartners: [], topics: ["Education & Skilling","AI Governance & Regulation","Enterprise & Industry AI"], dayCode: "Mon16" },
  { title: "\"sp\": \"Ashish Kulkarni, Indian Institute of Creative Technologies and National Chairman - AVGC-XR Forum | Pioneer of India's Creative Technology Ecosystem; Dr. Manish Kumar, NSDC, Championing AI First Solution for Skilling; Narayanan Ramaswamy, KPMG; Professor MS Vijay Kumar, MIT's Education Innovation Movement; Shankar Maruwada, EkStep Foundation\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5663\""], description: "\"sp\": \"Ashish Kulkarni, Indian Institute of Creative Technologies and National Chairman - AVGC-XR Forum | Pioneer of India's Creative Technology Ecosystem; Dr. Manish Kumar, NSDC, Championing AI First Solution for Skilling; Narayanan Ramaswamy, KPMG; Professor MS Vijay Kumar, MIT's Education Innovation Movement; Shankar Maruwada, EkStep Foundation\",", knowledgePartners: [], topics: ["Creative AI & Media","Education & Skilling"], dayCode: "Mon16" },
  { title: "\"t\": \"The Future of Employability in the Age of AI\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"The Future of Employability in the Age of AI\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"AI will create more job opportunities but also make many existing jobs redundant. This will also require skilling and re-skilling of workers across the spectrum. The Panel Discussion will focus on short-term and long-term impact of AI on employment, both at the industry level (e.g., manufacturing, healthcare, automobile) and function level (e.g., HR, sales, supply chain). Moreover, the panel will identify steps to be taken to mitigate the risks by enhancing opportunities.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 A\""], description: "\"d\": \"AI will create more job opportunities but also make many existing jobs redundant. This will also require skilling and re-skilling of workers across the spectrum. The Panel Discussion will focus on short-term and long-term impact of AI on employment, both at the industry level (e.g., manufacturing, healthcare, automobile) and function level (e.g., HR, sales, supply chain). Moreover, the panel will identify steps to be taken to mitigate the risks by enhancing opportunities.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Education & Skilling","Enterprise & Industry AI"], dayCode: "Mon16" },
