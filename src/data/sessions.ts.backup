  { title: "\"t\": \"AI Literacy: Building Skills, Inclusion, and Global Leadership\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI Literacy: Building Skills, Inclusion, and Global Leadership\",", knowledgePartners: [], topics: ["Women, Children & Social Impact","Global Cooperation & Diplomacy"], dayCode: "Mon16" },
  { title: "\"d\": \"This session addresses the critical AI literacy gap limiting equitable access to AI knowledge across sectors and geographies. It will explore strategies to embed AI education across formal learning, workforce upskilling, and lifelong learning through training multipliers like teachers, government officials, and workforce development leaders. It will introduce the concept of the Universal AI Literacy Stack, and examine how multi-stakeholder collaboration among governments, industry, academia, and global partners can position countries as AI literacy leaders.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 14\""], description: "\"d\": \"This session addresses the critical AI literacy gap limiting equitable access to AI knowledge across sectors and geographies. It will explore strategies to embed AI education across formal learning, workforce upskilling, and lifelong learning through training multipliers like teachers, government officials, and workforce development leaders. It will introduce the concept of the Universal AI Literacy Stack, and examine how multi-stakeholder collaboration among governments, industry, academia, and global partners can position countries as AI literacy leaders.\",", knowledgePartners: [], topics: ["Education & Skilling","Global Cooperation & Diplomacy","Enterprise & Industry AI"], dayCode: "Mon16" },
