  { title: "\"t\": \"Advancing AI Safety Across Languages, Cultures and Contexts\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Advancing AI Safety Across Languages, Cultures and Contexts\",", knowledgePartners: [], topics: ["Multilingual & Indic Language AI","AI Safety & Responsible AI"], dayCode: "Mon16" },
  { title: "\"d\": \"This panel discussion will explore why multilingual safety is essential for inclusive AI governance, addressing emerging risks such as jailbreaking in low-resource languages, and the current lack of culturally grounded benchmarks and evaluation frameworks. Participants will examine the challenges of building culturally sensitive safety datasets and ensuring that AI models can be developed, evaluated, and deployed safely across diverse linguistic and socio-cultural contexts, and highlight opportunities for collaborative research and open evaluation infrastructure.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 B\""], description: "\"d\": \"This panel discussion will explore why multilingual safety is essential for inclusive AI governance, addressing emerging risks such as jailbreaking in low-resource languages, and the current lack of culturally grounded benchmarks and evaluation frameworks. Participants will examine the challenges of building culturally sensitive safety datasets and ensuring that AI models can be developed, evaluated, and deployed safely across diverse linguistic and socio-cultural contexts, and highlight opportunities for collaborative research and open evaluation infrastructure.\",", knowledgePartners: [], topics: ["Multilingual & Indic Language AI","AI Governance & Regulation","AI Safety & Responsible AI","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
