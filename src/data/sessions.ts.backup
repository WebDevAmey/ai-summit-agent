  { title: "\"t\": \"Empowering the Next Generation of Entrepreneurs in Asia and India through AI\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Empowering the Next Generation of Entrepreneurs in Asia and India through AI\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
