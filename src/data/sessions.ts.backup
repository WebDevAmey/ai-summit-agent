  { title: "\"t\": \"From Insights to Action for Resilient, High-Performance Data Centres\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"From Insights to Action for Resilient, High-Performance Data Centres\",", knowledgePartners: [], topics: ["Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"d\": \"As AI-driven computing demand accelerates, data centers are emerging as complex, dynamic loads on modern power systems. This session examines how AI and advanced energy technologies are being deployed to enable resilient, flexible, and high-performance AI data center infrastructure. It features applied research about hybrid grid architectures, advanced cooling and geothermal systems, predictive digital twins, geospatial and transmission siting and planning tools, and the ARIES platform for large-scale testing and risk reduction.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 6\""], description: "\"d\": \"As AI-driven computing demand accelerates, data centers are emerging as complex, dynamic loads on modern power systems. This session examines how AI and advanced energy technologies are being deployed to enable resilient, flexible, and high-performance AI data center infrastructure. It features applied research about hybrid grid architectures, advanced cooling and geothermal systems, predictive digital twins, geospatial and transmission siting and planning tools, and the ARIES platform for large-scale testing and risk reduction.\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
