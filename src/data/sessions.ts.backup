  { title: "\"t\": \"Reimagining Gender in Technology - Designing Safer Digital Futures and Advancing Ethical AI for Inclusive Platforms\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Reimagining Gender in Technology - Designing Safer Digital Futures and Advancing Ethical AI for Inclusive Platforms\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"India is rapidly adopting AI and digital technologies across governance, education, health, commerce, and social life. While these systems offer inclusion and innovation, they also pose growing risks for women, girls, and gender-diverse persons, including cyberstalking, deepfakes, harassment, and algorithmic bias, limiting safe digital participation. As India's digital population expands, embedding gender-responsive, rights-based, and ethical approaches in digital governance and AI design is urgent.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 15\""], description: "\"d\": \"India is rapidly adopting AI and digital technologies across governance, education, health, commerce, and social life. While these systems offer inclusion and innovation, they also pose growing risks for women, girls, and gender-diverse persons, including cyberstalking, deepfakes, harassment, and algorithmic bias, limiting safe digital participation. As India's digital population expands, embedding gender-responsive, rights-based, and ethical approaches in digital governance and AI design is urgent.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Education & Skilling","Women, Children & Social Impact","AI Governance & Regulation"], dayCode: "Mon16" },
