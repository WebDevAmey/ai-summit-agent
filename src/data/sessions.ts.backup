  { title: "\"t\": \"Global Mission on AI for Energy Scaling through citizen-centric India Energy Stack\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Global Mission on AI for Energy Scaling through citizen-centric India Energy Stack\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability","Global Cooperation & Diplomacy"], dayCode: "Mon16" },
  { title: "\"d\": \"The world is entering a decisive decade for energy transition, with rapid solar growth and complex grids driving the need for AI-enabled digitalisation. India's experience, including the India Energy Stack, shows how scalable, inclusive digital public infrastructure can move from pilots to impact. Building on this the Ministry of Power, REC Ltd. and ISA are convening this session to advance a Global Mission on AI for Energy through collaboration and citizen-centric power systems.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L2 Audi 1\""], description: "\"d\": \"The world is entering a decisive decade for energy transition, with rapid solar growth and complex grids driving the need for AI-enabled digitalisation. India's experience, including the India Energy Stack, shows how scalable, inclusive digital public infrastructure can move from pilots to impact. Building on this the Ministry of Power, REC Ltd. and ISA are convening this session to advance a Global Mission on AI for Energy through collaboration and citizen-centric power systems.\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability","Global Cooperation & Diplomacy","Digital Public Infrastructure","AI Infrastructure & Compute"], dayCode: "Mon16" },
