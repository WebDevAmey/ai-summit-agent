  { title: "\"t\": \"AI for the Global South: From Governance to Inclusion\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI for the Global South: From Governance to Inclusion\",", knowledgePartners: [], topics: ["Women, Children & Social Impact","Global Cooperation & Diplomacy","AI Governance & Regulation"], dayCode: "Mon16" },
