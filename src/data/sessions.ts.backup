  { title: "\"t\": \"Towards Global Cooperation for Equitable Access to Healthcare AI\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Towards Global Cooperation for Equitable Access to Healthcare AI\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Global Cooperation & Diplomacy"], dayCode: "Mon16" },
  { title: "\"d\": \"Artificial Intelligence (AI) promises better healthcare worldwide, but biased datasets and designs risk reinforcing inequities and harming already marginalised communities (e.g. elderly, disabled, rural). An inter-disciplinary panel discussion will explore inclusive AI strategies (e.g. participatory design) that ensure AI is designed, developed, and deployed in a way that mitigates unintended harm and helps bridge health inequities.\",", dateLabel: "Tue, Feb 17", dateISO: "2026-02-17", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 14\""], description: "\"d\": \"Artificial Intelligence (AI) promises better healthcare worldwide, but biased datasets and designs risk reinforcing inequities and harming already marginalised communities (e.g. elderly, disabled, rural). An inter-disciplinary panel discussion will explore inclusive AI strategies (e.g. participatory design) that ensure AI is designed, developed, and deployed in a way that mitigates unintended harm and helps bridge health inequities.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Data Governance & Open Data"], dayCode: "Tue17" },
