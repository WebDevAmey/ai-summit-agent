  { title: "\"t\": \"MahaAI: Building Safe, Secure and Smart Governance\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"MahaAI: Building Safe, Secure and Smart Governance\",", knowledgePartners: [], topics: ["AI Governance & Regulation"], dayCode: "Mon16" },
  { title: "\"d\": \"This session will showcase Maharashtra's approach to building safe, secure and citizen-centric AI governance. It will examine how the State is embedding ethics, data protection, security and AI system for public services, safety and administration. The discussion will outline Maharashtra's policy roadmap for responsible AI adoption and highlight how AI can drive economic growth and social good while remaining transparent, accountable and trusted.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 10\""], description: "\"d\": \"This session will showcase Maharashtra's approach to building safe, secure and citizen-centric AI governance. It will examine how the State is embedding ethics, data protection, security and AI system for public services, safety and administration. The discussion will outline Maharashtra's policy roadmap for responsible AI adoption and highlight how AI can drive economic growth and social good while remaining transparent, accountable and trusted.\",", knowledgePartners: [], topics: ["Cybersecurity & National Security","AI Governance & Regulation","AI Safety & Responsible AI","Data Governance & Open Data"], dayCode: "Mon16" },
