  { title: "\"kp\": \"Karnataka Digital Economy Mission\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5966\""], description: "\"kp\": \"Karnataka Digital Economy Mission\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"t\": \"Capacity Building in Digital Health\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Capacity Building in Digital Health\",", knowledgePartners: [], topics: ["Healthcare & Biotech"], dayCode: "Mon16" },
  { title: "\"d\": \"Artificial Intelligence has rapidly achieved technical maturity across its first two developmental levels, yet organizational maturity has not kept pace. This panel examines the critical gap between AI capability and institutional readiness. It will explore how targeted capacity building, across leadership, workforce, governance, and processes, can unlock AI's full value. The discussion focuses on activating impact from the bottom of the value chain, ensuring AI adoption translates into measurable outcomes rather than isolated technological success.\",", dateLabel: "Tue, Feb 17", dateISO: "2026-02-17", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room No. 6\""], description: "\"d\": \"Artificial Intelligence has rapidly achieved technical maturity across its first two developmental levels, yet organizational maturity has not kept pace. This panel examines the critical gap between AI capability and institutional readiness. It will explore how targeted capacity building, across leadership, workforce, governance, and processes, can unlock AI's full value. The discussion focuses on activating impact from the bottom of the value chain, ensuring AI adoption translates into measurable outcomes rather than isolated technological success.\",", knowledgePartners: [], topics: ["AI Governance & Regulation"], dayCode: "Tue17" },
