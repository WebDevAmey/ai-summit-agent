  { title: "\"t\": \"AI and Children's Safety and Wellbeing\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI and Children's Safety and Wellbeing\",", knowledgePartners: [], topics: ["Women, Children & Social Impact","AI Safety & Responsible AI"], dayCode: "Mon16" },
  { title: "\"d\": \"Children are becoming among the earliest and most intensive users of AI, often before safeguards and regulations are fully in place. This session asks a direct question: how can AI be shaped to strengthen children's safety and wellbeing rather than undermine them? Drawing on technology leaders, child-rights experts, and a young AI innovator, the discussion examines advancing innovation, where risks demand firm limits, and how trust in AI can be built for the next generation.\",", dateLabel: "Thu, Feb 19", dateISO: "2026-02-19", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Bharat Mandapam\""], description: "\"d\": \"Children are becoming among the earliest and most intensive users of AI, often before safeguards and regulations are fully in place. This session asks a direct question: how can AI be shaped to strengthen children's safety and wellbeing rather than undermine them? Drawing on technology leaders, child-rights experts, and a young AI innovator, the discussion examines advancing innovation, where risks demand firm limits, and how trust in AI can be built for the next generation.\",", knowledgePartners: [], topics: ["Women, Children & Social Impact","AI Governance & Regulation","AI Safety & Responsible AI"], dayCode: "Thu19" },
