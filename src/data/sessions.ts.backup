  { title: "\"c2\": [\"healthcare\", \"climate\", \"education\"]", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5648\""], description: "\"c2\": [\"healthcare\", \"climate\", \"education\"]", knowledgePartners: [], topics: ["Healthcare & Biotech","Climate, Energy & Sustainability","Education & Skilling"], dayCode: "Mon16" },
  { title: "\"t\": \"Scaling Climate Solutions: Data, AI, and India's Energy Transformation\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Scaling Climate Solutions: Data, AI, and India's Energy Transformation\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"d\": \"Open data and AI can significantly strengthen climate resilience and help India meet its net zero target by 2070 and 500 GW renewable capacity by 2030. Achieving this requires faster digital transformation across climate and energy institutions and stronger Climate AI talent to break silos. This session convenes experts to examine the building blocks of a climate-energy data ecosystem, showcasing real-world use cases\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 A\""], description: "\"d\": \"Open data and AI can significantly strengthen climate resilience and help India meet its net zero target by 2070 and 500 GW renewable capacity by 2030. Achieving this requires faster digital transformation across climate and energy institutions and stronger Climate AI talent to break silos. This session convenes experts to examine the building blocks of a climate-energy data ecosystem, showcasing real-world use cases\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"sp\": \"Akhilesh Magal, Climate Dot Foundation; Dr. Cormekki Whitley, data.org; Dr. Priya Donti, MIT EECS and LIDS; Karan Shah, Artha India; Priyank Hirani, data.org; Srinivas Krishnaswamy, Vasudha Foundation; Swetha Ravi Kumar, FSR Global\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5708\""], description: "\"sp\": \"Akhilesh Magal, Climate Dot Foundation; Dr. Cormekki Whitley, data.org; Dr. Priya Donti, MIT EECS and LIDS; Karan Shah, Artha India; Priyank Hirani, data.org; Srinivas Krishnaswamy, Vasudha Foundation; Swetha Ravi Kumar, FSR Global\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability","Global Cooperation & Diplomacy","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"t\": \"Beyond Technicality: Multidisciplinary AI Governance for Real-World Impact\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Beyond Technicality: Multidisciplinary AI Governance for Real-World Impact\",", knowledgePartners: [], topics: ["AI Governance & Regulation"], dayCode: "Mon16" },
  { title: "\"d\": \"This interactive roundtable broadens the AI safety conversation by foregrounding societal, institutional, and ethical dimensions beyond purely technical approaches. It treats AI governance as a multidisciplinary, context-aware challenge rooted in real deployment settings and community experience. Through brief interventions and guided discussion, participants will identify shared priorities and co-develop principles and collaboration areas to embed interdisciplinary expertise, civil society voices, and context-sensitive evaluation into national and global AI policy frameworks.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 9\""], description: "\"d\": \"This interactive roundtable broadens the AI safety conversation by foregrounding societal, institutional, and ethical dimensions beyond purely technical approaches. It treats AI governance as a multidisciplinary, context-aware challenge rooted in real deployment settings and community experience. Through brief interventions and guided discussion, participants will identify shared priorities and co-develop principles and collaboration areas to embed interdisciplinary expertise, civil society voices, and context-sensitive evaluation into national and global AI policy frameworks.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Governance & Regulation","AI Safety & Responsible AI"], dayCode: "Mon16" },
