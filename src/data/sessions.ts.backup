  { title: "\"t\": \"Artificial General Intelligence: A New Paradigm of Safety, Security, Privacy, Ethics, and Governance\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Artificial General Intelligence: A New Paradigm of Safety, Security, Privacy, Ethics, and Governance\",", knowledgePartners: [], topics: ["Cybersecurity & National Security","AI Governance & Regulation","AI Safety & Responsible AI"], dayCode: "Mon16" },
  { title: "\"d\": \"Artificial general intelligence will intensify existing challenges around safety, security, privacy, ethics, and governance. This session will examine how current trajectories in advanced AI raise near-term governance questions, particularly for developing economies. It will explore institutional readiness, policy gaps, and risk pathways across data, compute, markets, and deployment contexts, and will surface practical considerations for anticipatory governance that can inform responsible decision-making before irreversible dependencies take hold.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Nalanda Banquet\""], description: "\"d\": \"Artificial general intelligence will intensify existing challenges around safety, security, privacy, ethics, and governance. This session will examine how current trajectories in advanced AI raise near-term governance questions, particularly for developing economies. It will explore institutional readiness, policy gaps, and risk pathways across data, compute, markets, and deployment contexts, and will surface practical considerations for anticipatory governance that can inform responsible decision-making before irreversible dependencies take hold.\",", knowledgePartners: [], topics: ["Cybersecurity & National Security","AI Governance & Regulation","AI Safety & Responsible AI","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
