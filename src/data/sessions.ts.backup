  { title: "\"t\": \"From Guardrails to Adaptive Governance in the Global South: Building a Joint Responsible Governance‚ÄìHTA Framework to Deliver Safe, Trusted, and Scalable AI in Health\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"From Guardrails to Adaptive Governance in the Global South: Building a Joint Responsible Governance‚ÄìHTA Framework to Deliver Safe, Trusted, and Scalable AI in Health\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Global Cooperation & Diplomacy","AI Governance & Regulation","AI Safety & Responsible AI"], dayCode: "Mon16" },
