  { title: "\"t\": \"AI for Her: Catalyst Women Leaders Driving Responsible and Inclusive AI\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI for Her: Catalyst Women Leaders Driving Responsible and Inclusive AI\",", knowledgePartners: [], topics: ["Women, Children & Social Impact","AI Safety & Responsible AI"], dayCode: "Mon16" },
  { title: "\"d\": \"The session will cover responsible AI frameworks, mitigation of gender and data biases, and the translation of national AI policies into real-world applications across health, education, and social sectors. It will also address access to capital, mentorship, and ecosystem support required to scale women-led AI innovations. Through a moderated panel discussion, global technology leaders, policymakers, and founders will share practical insights on building safe and trusted AI systems while nurturing future women technologists.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Shakuntalam Banquet\""], description: "\"d\": \"The session will cover responsible AI frameworks, mitigation of gender and data biases, and the translation of national AI policies into real-world applications across health, education, and social sectors. It will also address access to capital, mentorship, and ecosystem support required to scale women-led AI innovations. Through a moderated panel discussion, global technology leaders, policymakers, and founders will share practical insights on building safe and trusted AI systems while nurturing future women technologists.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Education & Skilling","Women, Children & Social Impact","Global Cooperation & Diplomacy","AI Governance & Regulation","AI Safety & Responsible AI","Data Governance & Open Data"], dayCode: "Mon16" },
