  { title: "\"t\": \"Democratizing AI for the Last Mile:Language, Access and Trust at Scale\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Democratizing AI for the Last Mile:Language, Access and Trust at Scale\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","Multilingual & Indic Language AI"], dayCode: "Mon16" },
  { title: "\"d\": \"The session advances the conversation from democratizing AI access to enabling meaningful last-mile adoption, where inclusion, usability, and public trust define real impact. It explores how language AI and foundation models can serve as shared national digital infrastructure, powering governance and cultural services at scale. Emphasizing trust-by-design, it highlights transparent, accountable, human-centric AI and collaboration across industry, government platforms, and public institutions to responsibly scale inclusive AI.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 A\""], description: "\"d\": \"The session advances the conversation from democratizing AI access to enabling meaningful last-mile adoption, where inclusion, usability, and public trust define real impact. It explores how language AI and foundation models can serve as shared national digital infrastructure, powering governance and cultural services at scale. Emphasizing trust-by-design, it highlights transparent, accountable, human-centric AI and collaboration across industry, government platforms, and public institutions to responsibly scale inclusive AI.\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","Multilingual & Indic Language AI","Frontier AI & Foundation Models","Women, Children & Social Impact","AI Governance & Regulation","AI Infrastructure & Compute","Enterprise & Industry AI"], dayCode: "Mon16" },
