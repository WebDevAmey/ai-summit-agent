  { title: "\"t\": \"Data Governance at the Intersection of Digital Public Infrastructure and Artificial Intelligence\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Data Governance at the Intersection of Digital Public Infrastructure and Artificial Intelligence\",", knowledgePartners: [], topics: ["AI Governance & Regulation","Digital Public Infrastructure","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"d\": \"Digital public infrastructures (DPIs) such as national digital identity systems, payments rails, data-trust frameworks and shared open datasets are rapidly becoming the foundation for AI  development and deployment. When AI systems are trained on or linked through DPIs, the governance of data becomes critical: who collects data, how it's aggregated, for what purposes, with what safeguards.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Shakuntalam Banquet\""], description: "\"d\": \"Digital public infrastructures (DPIs) such as national digital identity systems, payments rails, data-trust frameworks and shared open datasets are rapidly becoming the foundation for AI  development and deployment. When AI systems are trained on or linked through DPIs, the governance of data becomes critical: who collects data, how it's aggregated, for what purposes, with what safeguards.\",", knowledgePartners: [], topics: ["AI Governance & Regulation","Digital Public Infrastructure","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
