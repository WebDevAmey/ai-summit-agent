  { title: "\"t\": \"From Co-Design to Courtroom: Building a Fair Trial Adviser for Justice Systems\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"From Co-Design to Courtroom: Building a Fair Trial Adviser for Justice Systems\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
