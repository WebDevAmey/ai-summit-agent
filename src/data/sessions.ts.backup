  { title: "\"t\": \"Stakeholder Workshop (World Economic Forum): Advancing Safe, Secure and Trustworthy AI Governance\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Stakeholder Workshop (World Economic Forum): Advancing Safe, Secure and Trustworthy AI Governance\",", knowledgePartners: [], topics: ["AI Governance & Regulation"], dayCode: "Mon16" },
  { title: "\"d\": \"This semi-private stakeholder dialogue will convene senior public- and private-sector leaders for a focused discussion on advancing practical AI governance. Building on the principles of the Hiroshima AI Process and other global frameworks, the session will explore how shared commitments to safe, secure, and trustworthy AI can be translated into actionable, regionally relevant governance approaches that enable innovation and protect societal interests, as well as strategies for building resilient AI innovation ecosystems through regional collaboration.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 15\""], description: "\"d\": \"This semi-private stakeholder dialogue will convene senior public- and private-sector leaders for a focused discussion on advancing practical AI governance. Building on the principles of the Hiroshima AI Process and other global frameworks, the session will explore how shared commitments to safe, secure, and trustworthy AI can be translated into actionable, regionally relevant governance approaches that enable innovation and protect societal interests, as well as strategies for building resilient AI innovation ecosystems through regional collaboration.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Governance & Regulation"], dayCode: "Mon16" },
  { title: "\"sp\": \"Agustina Callegari, World Economic Forum; Cathy Li, World Economic Forum; Samira Gazzane, World Economic Forum\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5602\""], description: "\"sp\": \"Agustina Callegari, World Economic Forum; Cathy Li, World Economic Forum; Samira Gazzane, World Economic Forum\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"t\": \"Small AI for Big Impact\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Small AI for Big Impact\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"Small AI is essential for scaling solutions in the Global South via data-efficient computing, low-resource datasets, and localized edge inference. Grounded in responsible, local contexts, this session provides a roadmap for ML practitioners in India and beyond to achieve scale. Through a technical lens, we examine how affordable, scalable AI deployment can advance UN Sustainable Development Goals (SDGs), offering global leadership and technical insights into driving meaningful social impact through localized, accessible technology.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 10\""], description: "\"d\": \"Small AI is essential for scaling solutions in the Global South via data-efficient computing, low-resource datasets, and localized edge inference. Grounded in responsible, local contexts, this session provides a roadmap for ML practitioners in India and beyond to achieve scale. Through a technical lens, we examine how affordable, scalable AI deployment can advance UN Sustainable Development Goals (SDGs), offering global leadership and technical insights into driving meaningful social impact through localized, accessible technology.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Safety & Responsible AI","Data Governance & Open Data"], dayCode: "Mon16" },
