  { title: "\"t\": \"Payments to Commerce: From the lens of People, Planet, and Progress\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Payments to Commerce: From the lens of People, Planet, and Progress\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"Digital payments transformed economies by making transactions faster, safer, and more inclusive‚Äîwith AI at their core. This panel brings together industry and academic leaders who helped shape that journey and now turn their lens from payments to commerce. As commerce enters its next evolution, the discussion explores implications for people, planet, and progress‚Äîfrom trust and inclusion to global trade and sustainability‚Äîand how enterprises, governments, and academia can prepare for an AI-native future.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Chanakya Auditorium\""], description: "\"d\": \"Digital payments transformed economies by making transactions faster, safer, and more inclusive‚Äîwith AI at their core. This panel brings together industry and academic leaders who helped shape that journey and now turn their lens from payments to commerce. As commerce enters its next evolution, the discussion explores implications for people, planet, and progress‚Äîfrom trust and inclusion to global trade and sustainability‚Äîand how enterprises, governments, and academia can prepare for an AI-native future.\",", knowledgePartners: [], topics: ["Climate, Energy & Sustainability","Women, Children & Social Impact","Global Cooperation & Diplomacy","Enterprise & Industry AI"], dayCode: "Mon16" },
