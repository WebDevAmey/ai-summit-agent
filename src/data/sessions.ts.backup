  { title: "\"t\": \"AI as an Opportunity for More Impactful Open Data\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI as an Opportunity for More Impactful Open Data\",", knowledgePartners: [], topics: ["Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"d\": \"This session explores how AI can strengthen open public data systems to deliver greater social and economic value. The session explores the policy, institutional, and technical conditions needed for AI-ready data ecosystems, highlighting the role of National Statistical Offices, transparency, and collaboration in building trust, while simultaneously improving data quality.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 15\""], description: "\"d\": \"This session explores how AI can strengthen open public data systems to deliver greater social and economic value. The session explores the policy, institutional, and technical conditions needed for AI-ready data ecosystems, highlighting the role of National Statistical Offices, transparency, and collaboration in building trust, while simultaneously improving data quality.\",", knowledgePartners: [], topics: ["AI Governance & Regulation","Data Governance & Open Data"], dayCode: "Mon16" },
