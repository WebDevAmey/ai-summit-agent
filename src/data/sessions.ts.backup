  { title: "\"t\": \"Women, Work, and the Future of AI\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Women, Work, and the Future of AI\",", knowledgePartners: [], topics: ["Women, Children & Social Impact"], dayCode: "Mon16" },
  { title: "\"d\": \"This session blends film, dialogue, and live demonstration to explore who shapes AI and on whose terms. Featuring a screening of 'Humans in the Loop', a moderated panel aligned with the India AI Impact Summit's chakra framework, and a demo of a Gender Bias Benchmarking tool, the session centers women's inclusion, data workers' visibility, local context, and dignified digital work as pillars of responsible AI.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 9\""], description: "\"d\": \"This session blends film, dialogue, and live demonstration to explore who shapes AI and on whose terms. Featuring a screening of 'Humans in the Loop', a moderated panel aligned with the India AI Impact Summit's chakra framework, and a demo of a Gender Bias Benchmarking tool, the session centers women's inclusion, data workers' visibility, local context, and dignified digital work as pillars of responsible AI.\",", knowledgePartners: [], topics: ["Hackathons, Demos & Masterclasses","Women, Children & Social Impact","AI Safety & Responsible AI","Data Governance & Open Data"], dayCode: "Mon16" },
