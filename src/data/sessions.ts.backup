  { title: "\"t\": \"The Sociotechnical Turn in AI Governance: Bridging Theory and Practice\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"The Sociotechnical Turn in AI Governance: Bridging Theory and Practice\",", knowledgePartners: [], topics: ["AI Governance & Regulation"], dayCode: "Mon16" },
