  { title: "\"c2\": [\"healthcare\", \"education\", \"dpi\"]", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5646\""], description: "\"c2\": [\"healthcare\", \"education\", \"dpi\"]", knowledgePartners: [], topics: ["Healthcare & Biotech","Education & Skilling","Digital Public Infrastructure"], dayCode: "Mon16" },
  { title: "\"t\": \"Rightsizing Governance in an AI-Driven World\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Rightsizing Governance in an AI-Driven World\",", knowledgePartners: [], topics: ["AI Governance & Regulation"], dayCode: "Mon16" },
  { title: "\"d\": \"This panel explores how India and partner nations can use AI for real-world impact, bridge digital divides, and support inclusive growth. Aligned with the UK‚ÄìIndia strategic tech partnership, it shows how global forums can turn shared ambition into action on AI governance and deployment. The discussion covers shared compute and data, support for smaller states, and high-impact collaboration across healthcare, education, and climate for responsible AI at scale.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 B\""], description: "\"d\": \"This panel explores how India and partner nations can use AI for real-world impact, bridge digital divides, and support inclusive growth. Aligned with the UK‚ÄìIndia strategic tech partnership, it shows how global forums can turn shared ambition into action on AI governance and deployment. The discussion covers shared compute and data, support for smaller states, and high-impact collaboration across healthcare, education, and climate for responsible AI at scale.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Climate, Energy & Sustainability","Education & Skilling","Global Cooperation & Diplomacy","AI Governance & Regulation","AI Safety & Responsible AI","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Mon16" },
