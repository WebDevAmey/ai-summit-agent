  { title: "\"kp\": \"DIT, Government of Maharashtra\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5759\""], description: "\"kp\": \"DIT, Government of Maharashtra\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"t\": \"AI at the Core, 6G at the Edge: Designing India's Next Resilient, Innovative and Efficient Digital Frontier\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"AI at the Core, 6G at the Edge: Designing India's Next Resilient, Innovative and Efficient Digital Frontier\",", knowledgePartners: [], topics: ["Frontier AI & Foundation Models"], dayCode: "Mon16" },
  { title: "\"d\": \"The panel discussion situates India's opportunity at the intersection of these two forces: AI providing cognition, autonomy, and predictive intelligence, while 6G provides real-time sensing, extreme reliability, and pervasive connectivity across terrestrial, aerial, maritime, and satellite systems. In this emerging paradigm, networks themselves become AI-native‚Äîthe radio interface, the RAN, the core, and the edge begin to reason, adapt, self-organize, and optimize through multi-agent intelligence\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Chanakya Auditorium\""], description: "\"d\": \"The panel discussion situates India's opportunity at the intersection of these two forces: AI providing cognition, autonomy, and predictive intelligence, while 6G provides real-time sensing, extreme reliability, and pervasive connectivity across terrestrial, aerial, maritime, and satellite systems. In this emerging paradigm, networks themselves become AI-native‚Äîthe radio interface, the RAN, the core, and the edge begin to reason, adapt, self-organize, and optimize through multi-agent intelligence\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"sp\": \"Radhakant Das, Tata Consultancy Services; Rajesh Kumar Pathak, Bharat 6G Alliance, New Delhi; Sandeep Sharma, Tech Mahindra; Sanjay Nekkanti, Dhruva Space Pvt. Ltd.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5772\""], description: "\"sp\": \"Radhakant Das, Tata Consultancy Services; Rajesh Kumar Pathak, Bharat 6G Alliance, New Delhi; Sandeep Sharma, Tech Mahindra; Sanjay Nekkanti, Dhruva Space Pvt. Ltd.\",", knowledgePartners: [], topics: ["Space, Quantum & Emerging Tech"], dayCode: "Mon16" },
  { title: "\"t\": \"Blueprint to Accelerate India's AI Success\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Blueprint to Accelerate India's AI Success\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"India is at a pivotal stage in its AI journey, aiming to become a global innovation hub. This session will unveil the \\\"Blueprint to Accelerate India's AI Growth\\\", outlining priorities across Invest, Innovate, and Evolve; scaling infrastructure, building skills, enabling indigenous innovation, and ensuring trusted governance. The session will convene leaders to discuss how the Blueprint can advance AI readiness, drive economic growth, and promote social good, supporting India's vision for inclusive and sustainable AI leadership.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 6\""], description: "\"d\": \"India is at a pivotal stage in its AI journey, aiming to become a global innovation hub. This session will unveil the \\\"Blueprint to Accelerate India's AI Growth\\\", outlining priorities across Invest, Innovate, and Evolve; scaling infrastructure, building skills, enabling indigenous innovation, and ensuring trusted governance. The session will convene leaders to discuss how the Blueprint can advance AI readiness, drive economic growth, and promote social good, supporting India's vision for inclusive and sustainable AI leadership.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Governance & Regulation","AI Infrastructure & Compute"], dayCode: "Mon16" },
  { title: "\"sp\": \"Hon'ble Shri Jayant Chaudhary Ji, GoI; Manish Gupta, Dell Technologies India; Mridu Bhandari, Network18; Rajgopal A S, NxtGen Cloud Technologies; Rajiv Memani, EY; Rishi Bal, BharatGen; Vivek Mohindra, Dell Technologies Global\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"5802\""], description: "\"sp\": \"Hon'ble Shri Jayant Chaudhary Ji, GoI; Manish Gupta, Dell Technologies India; Mridu Bhandari, Network18; Rajgopal A S, NxtGen Cloud Technologies; Rajiv Memani, EY; Rishi Bal, BharatGen; Vivek Mohindra, Dell Technologies Global\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy"], dayCode: "Mon16" },
  { title: "\"t\": \"Scaling Human Potential in the Age of AI\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Scaling Human Potential in the Age of AI\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"AI is rapidly becoming a foundational layer across education, creativity, and enterprise. This session examines how human potential is shaped, scaled, or constrained by AI systems and what it takes to ensure these technologies expand capability rather than concentrate advantage. Leaders from automation, AI-driven enterprise, data ecosystems, global business, philanthropy, and the creative arts discuss where AI meaningfully augments human skill, and where intentional design is required to prevent new forms of exclusion.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 8\""], description: "\"d\": \"AI is rapidly becoming a foundational layer across education, creativity, and enterprise. This session examines how human potential is shaped, scaled, or constrained by AI systems and what it takes to ensure these technologies expand capability rather than concentrate advantage. Leaders from automation, AI-driven enterprise, data ecosystems, global business, philanthropy, and the creative arts discuss where AI meaningfully augments human skill, and where intentional design is required to prevent new forms of exclusion.\",", knowledgePartners: [], topics: ["Creative AI & Media","Education & Skilling","Global Cooperation & Diplomacy","Data Governance & Open Data","Enterprise & Industry AI"], dayCode: "Mon16" },
