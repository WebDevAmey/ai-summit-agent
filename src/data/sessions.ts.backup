  { title: "\"t\": \"Embedding Trust in Innovation: AI Governance and Quality Infrastructure for Growth\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Embedding Trust in Innovation: AI Governance and Quality Infrastructure for Growth\",", knowledgePartners: [], topics: ["AI Governance & Regulation","AI Infrastructure & Compute"], dayCode: "Mon16" },
  { title: "\"d\": \"This roundtable convenes policymakers, regulators, industry leaders, and global experts to shape trusted pathways for AI adoption. It examines how AI-enabled Quality Infrastructure can strengthen governance, accountability, and global acceptance. Through applied use cases and international perspectives, it highlights interoperable assurance frameworks that reduce compliance friction, boost MSME competitiveness, enable cross-border trade, and position India as a leader in responsible, innovation-driven AI growth for inclusive, secure, and scalable digital transformation.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 B\""], description: "\"d\": \"This roundtable convenes policymakers, regulators, industry leaders, and global experts to shape trusted pathways for AI adoption. It examines how AI-enabled Quality Infrastructure can strengthen governance, accountability, and global acceptance. Through applied use cases and international perspectives, it highlights interoperable assurance frameworks that reduce compliance friction, boost MSME competitiveness, enable cross-border trade, and position India as a leader in responsible, innovation-driven AI growth for inclusive, secure, and scalable digital transformation.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Governance & Regulation","AI Safety & Responsible AI","AI Infrastructure & Compute","Enterprise & Industry AI"], dayCode: "Mon16" },
