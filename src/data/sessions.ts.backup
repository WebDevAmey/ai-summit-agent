  { title: "\"t\": \"From Policy to Harvest: Leveraging Generative AI for Data-Driven Agricultural Transformation\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"From Policy to Harvest: Leveraging Generative AI for Data-Driven Agricultural Transformation\",", knowledgePartners: [], topics: ["Agriculture & Food Systems","AI Governance & Regulation","Data Governance & Open Data"], dayCode: "Mon16" },
  { title: "\"d\": \"The session will bring together experts to spotlight India's next-generation digital agriculture infrastructure and examine how Generative AI can translate policy intent into on-ground agricultural productivity, resilience, and sustainability. The discussion will focus on the role of AI in enabling precision farming, improving crop forecasting, enhancing climate risk modelling, optimising supply chains, and strengthening farmer advisory services through real-time, data-driven insights.\",", dateLabel: "Tue, Feb 17", dateISO: "2026-02-17", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 15\""], description: "\"d\": \"The session will bring together experts to spotlight India's next-generation digital agriculture infrastructure and examine how Generative AI can translate policy intent into on-ground agricultural productivity, resilience, and sustainability. The discussion will focus on the role of AI in enabling precision farming, improving crop forecasting, enhancing climate risk modelling, optimising supply chains, and strengthening farmer advisory services through real-time, data-driven insights.\",", knowledgePartners: [], topics: ["Agriculture & Food Systems","Climate, Energy & Sustainability","AI Governance & Regulation","AI Infrastructure & Compute","Data Governance & Open Data"], dayCode: "Tue17" },
