  { title: "\"t\": \"Digital Ministers' / Senior Officials' Roundtable on the International AI Safety Report\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Digital Ministers' / Senior Officials' Roundtable on the International AI Safety Report\",", knowledgePartners: [], topics: ["AI Safety & Responsible AI"], dayCode: "Mon16" },
  { title: "\"d\": \"AI capabilities outpace the evidence-base on impacts and safeguards, creating a dilemma for policymakers: How to act early enough to reduce risk while ensuring progress of effective approaches? Global experts will present and discuss the key findings of the International AI Safety Report, an initiative backed by 30+ countries and international organizations, providing a science-based understanding of general-purpose AI capabilities, emerging risks, and mitigation measures.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 15\""], description: "\"d\": \"AI capabilities outpace the evidence-base on impacts and safeguards, creating a dilemma for policymakers: How to act early enough to reduce risk while ensuring progress of effective approaches? Global experts will present and discuss the key findings of the International AI Safety Report, an initiative backed by 30+ countries and international organizations, providing a science-based understanding of general-purpose AI capabilities, emerging risks, and mitigation measures.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Governance & Regulation","AI Safety & Responsible AI"], dayCode: "Mon16" },
