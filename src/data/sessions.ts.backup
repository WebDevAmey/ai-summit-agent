  { title: "\"t\": \"Trustworthy AI: Balancing Innovation and Regulation\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Trustworthy AI: Balancing Innovation and Regulation\",", knowledgePartners: [], topics: ["AI Governance & Regulation"], dayCode: "Mon16" },
  { title: "\"d\": \"This panel will explore how innovation and regulation can evolve together as artificial intelligence advances. It will examine trust, transparency, accountability, and responsible development, highlighting how these principles may encourage sustainable, human-centric AI adoption within an increasingly interconnected ecosystem.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L2 Audi 1\""], description: "\"d\": \"This panel will explore how innovation and regulation can evolve together as artificial intelligence advances. It will examine trust, transparency, accountability, and responsible development, highlighting how these principles may encourage sustainable, human-centric AI adoption within an increasingly interconnected ecosystem.\",", knowledgePartners: [], topics: ["AI Governance & Regulation","AI Safety & Responsible AI"], dayCode: "Mon16" },
