  { title: "\"t\": \"Towards a Safer South: Launch of the Global South Network on AI Safety and Evaluation\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Towards a Safer South: Launch of the Global South Network on AI Safety and Evaluation\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Safety & Responsible AI"], dayCode: "Mon16" },
  { title: "\"d\": \"The session will launch the Global South Network on AI Safety & Evaluation ‚Äî a global alliance of research and civil society organisations working to advance context-aware AI safety research and practice rooted in realities of the Global South. It will explore how AI safety priorities differ across Global South contexts, where South-South collaboration is most needed, and how evidence from deployment contexts across the Global South can strengthen global AI safety efforts.\",", dateLabel: "Wed, Feb 18", dateISO: "2026-02-18", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"Bharat Mandapam\""], description: "\"d\": \"The session will launch the Global South Network on AI Safety & Evaluation ‚Äî a global alliance of research and civil society organisations working to advance context-aware AI safety research and practice rooted in realities of the Global South. It will explore how AI safety priorities differ across Global South contexts, where South-South collaboration is most needed, and how evidence from deployment contexts across the Global South can strengthen global AI safety efforts.\",", knowledgePartners: [], topics: ["Global Cooperation & Diplomacy","AI Safety & Responsible AI"], dayCode: "Wed18" },
