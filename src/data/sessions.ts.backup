  { title: "\"t\": \"Beyond the Cloud: The Sovereign AI Moment\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Beyond the Cloud: The Sovereign AI Moment\",", knowledgePartners: [], topics: ["General AI"], dayCode: "Mon16" },
  { title: "\"d\": \"While much of the world defaults to cloud-hosted AI, a growing cohort of  enterprises and public sector institutions are charting a different course ‚Äî deploying powerful AI models on their own infrastructure, on their own terms.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"L1 Meeting Room No. 6\""], description: "\"d\": \"While much of the world defaults to cloud-hosted AI, a growing cohort of  enterprises and public sector institutions are charting a different course ‚Äî deploying powerful AI models on their own infrastructure, on their own terms.\",", knowledgePartners: [], topics: ["AI Infrastructure & Compute","Enterprise & Industry AI"], dayCode: "Mon16" },
