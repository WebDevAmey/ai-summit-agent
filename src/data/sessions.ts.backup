  { title: "\"t\": \"Empowering Communities in the Age of Advanced AI: Inclusion and Safety for Sustainable Development\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "9:00 AM", endTime: "", location: "Bharat Mandapam", room: "Main Hall", speakers: [], description: "\"t\": \"Empowering Communities in the Age of Advanced AI: Inclusion and Safety for Sustainable Development\",", knowledgePartners: [], topics: ["Women, Children & Social Impact","AI Safety & Responsible AI"], dayCode: "Mon16" },
  { title: "\"d\": \"As AI becomes central to agriculture, health, social protection, and digital public infrastructure, safety and inclusion extend beyond technical reliability to equity, agency, deception, and power imbalances. This session positions AI safety as essential to sustainable development in the Global South, ensuring technologies respect local contexts and reduce inequality. Participants will examine equity, deception and manipulation risks, and how Global South perspectives can shape proactive, globally relevant AI safety frameworks before large-scale deployment begins worldwide.\",", dateLabel: "Mon, Feb 16", dateISO: "2026-02-16", startTime: "8:26 PM", endTime: "2:00 AM", location: "Bharat Mandapam", room: "Main Hall", speakers: ["\"West Wing Room 4 A\""], description: "\"d\": \"As AI becomes central to agriculture, health, social protection, and digital public infrastructure, safety and inclusion extend beyond technical reliability to equity, agency, deception, and power imbalances. This session positions AI safety as essential to sustainable development in the Global South, ensuring technologies respect local contexts and reduce inequality. Participants will examine equity, deception and manipulation risks, and how Global South perspectives can shape proactive, globally relevant AI safety frameworks before large-scale deployment begins worldwide.\",", knowledgePartners: [], topics: ["Healthcare & Biotech","Agriculture & Food Systems","Women, Children & Social Impact","Global Cooperation & Diplomacy","AI Safety & Responsible AI","Digital Public Infrastructure","AI Infrastructure & Compute"], dayCode: "Mon16" },
